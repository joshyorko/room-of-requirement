{
  "kind": {
    "repo": "kubernetes-sigs/kind",
    "source": "release",
    "max_major": null,
    "include_prerelease": false,
    "download_url_template": "https://kind.sigs.k8s.io/dl/v{version}/kind-linux-amd64",
    "targets": [
      {
        "file": ".devcontainer/Dockerfile",
        "patterns": [
          "ARG KIND_VERSION=v(?P<version>[0-9]+\\.[0-9]+(?:\\.[0-9]+)?)"
        ],
        "sha256_pattern": "ARG KIND_SHA256=(?P<sha256>\\S+)"
      }
    ]
  },
